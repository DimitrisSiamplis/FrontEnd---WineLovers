ategoriesResults = () => {\r\n    let choiceDetails = {\r\n      choice: choice,\r\n    };\r\n    fetch(\"https://winelovers.herokuapp.com/bigCaregories\", {\r\n      method: \"POST\",\r\n      body: JSON.stringify(choiceDetails),\r\n      headers: { \"Content-Type\": \"application/json\" },\r\n    })\r\n      .then((res) => res.json())\r\n      .then((json) => {\r\n        console.log(json);\r\n        console.log(\r\n          json.opinion.filter(function (e) {\r\n            return e.Color == \"red\";\r\n          }).length\r\n        );\r\n        setResults(json);\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    getBigCategoriesResults();\r\n  }, []);\r\n\r\n  const dataType = [\r\n    [\"Type\", \"Type per Wine\"],\r\n    [\r\n      \"Dry\",\r\n      results.length !== 0\r\n        ? results.opinion.filter(function (e) {\r\n            return e.Type == \"Ξηρό\";\r\n          }).length\r\n        : 0,\r\n    ],\r\n    [\r\n      \"Semi Dry\",\r\n      results.length !== 0\r\n        ? results.opinion.filter(function (e) {\r\n            return e.Type == \"semi dry\";\r\n          }).length\r\n        : 0,\r\n    ],\r\n    [\r\n      \"Semi Sweet\",\r\n      results.length !== 0\r\n        ? results.opinion.filter(function (e) {\r\n            return e.Type == \"semi sweet\";\r\n          }).length\r\n        : 0,\r\n    ],\r\n    [\r\n      \"Sweet\",\r\n      results.length !== 0\r\n        ? results.opinion.filter(function (e) {\r\n            return e.Type == \"sweet\";\r\n          }).length\r\n        : 0,\r\n    ],\r\n  ];\r\n\r\n  const optionsType = {\r\n    title: `Wine & Type`,\r\n    colors: [\"#1f77b4\", \"#ff7f6e\", \"#d62728\", \"#2ca02c\"],\r\n  };\r\n\r\n  // ------------ COLOR -----------------------\r\n  const dataColor = [\r\n    [\"Type\", \"Type per Wine\"],\r\n    [\r\n      \"Red\",\r\n      results.length !== 0\r\n        ? results.opinion.filter(function (e) {\r\n            return e.Color == \"red\";\r\n          }).length\r\n   