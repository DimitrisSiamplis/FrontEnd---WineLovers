sName=\"login\" handleCallback={handleCallback} />\n                </Col>\n                <Col xs={4}></Col>\n              </>\n            )}\n\n            {loginOrRegister === \"register\" && (\n              <>\n                <Col xs={3}></Col>\n                <Col xs={deviceSize < 900 ? (deviceSize < 600 ? 10 : 6) : 6}>\n                  <Register />\n                </Col>\n              </>\n            )}\n          </Row>\n          <div className=\"checked-login-register\">\n            <strong\n            className=\"login-register-text\"\n              onClick={() => {\n                setLoginOrRegister(\n                  loginOrRegister === \"login\" ? \"register\" : \"login\"\n                );\n              }}\n            >\n              {loginOrRegister === \"login\" ? \"Regisert\" : \"Login\"}\n              {loginOrRegister === \"login\" ? (\n                <AiOutlineUserAdd className=\"login-register-icons\" />\n              ) : (\n                <BiLogIn className=\"login-register-icons\" />\n              )}\n            </strong>\n          </div>\n        </Container>\n      )}\n\n      {/* <br />\n      <br />\n      <br />\n      <br />\n      <br /> */}\n      <Footer />\n    </div>\n  );\n}\n\nexport default App;\n","import ReactDOM from 'react-dom';\nimport { BrowserRouter } from 'react-router-dom';\n\nimport './index.css';\nimport App from './App';\n\nReactDOM.render( <BrowserRouter>  <App /> </BrowserRouter>, document.getElementById('root'));\n","// extracted by mini-css-extract-plugin"]}}