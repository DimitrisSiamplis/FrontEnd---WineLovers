   Search{\" \"}\r\n                                  {wines.length !== 0 && (\r\n                                    <p>({wines[0].wines.length}&nbsp;wines)</p>\r\n                                  )}\r\n                                </Button>\r\n                              </Col>\r\n                            </Row>\r\n                            &nbsp;\r\n                          </Form>\r\n                        </Row>\r\n                      </Container>\r\n                    </div>\r\n                  </div>\r\n                )}\r\n                {deviceSize < 1200 && showFilter && (\r\n                  <>\r\n                    {\" \"}\r\n                    <br /> <br /> <br />\r\n                  </>\r\n                )}\r\n              </>\r\n            </Col>\r\n            <Col\r\n              xs={\r\n                showFilter\r\n                  ? deviceSize < 1300\r\n                    ? deviceSize < 900\r\n                      ? deviceSize < 800\r\n                        ? 12\r\n                        : 7\r\n                      : 8\r\n                    : 9\r\n                  : 12\r\n              }\r\n            >\r\n              <>\r\n                <div className=\"container\">\r\n                  <Container>\r\n                    {activeSpinner && (\r\n                      <Spinner\r\n                        className=\"spinner\"\r\n                        animation=\"border\"\r\n                        variant=\"primary\"\r\n                      />\r\n                    )}\r\n                    {wines.length !== 0 && (\r\n                      <>\r\n                        <Row>\r\n                          <div className=\"pagination-center\">\r\n                            <div className=\"box-pagination\">\r\n                              <div className=\"page\">\r\n                                <p\r\n                                  onClick={() => {\r\n                                    clickOnPrevPag