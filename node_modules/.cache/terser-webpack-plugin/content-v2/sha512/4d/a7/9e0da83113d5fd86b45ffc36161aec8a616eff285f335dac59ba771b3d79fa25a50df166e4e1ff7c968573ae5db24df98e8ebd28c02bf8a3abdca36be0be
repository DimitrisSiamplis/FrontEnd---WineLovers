elected, setOrderSelected] = useState([]);\r\n  const [show, setShow] = useState(false);\r\n  const handleClose = () => setShow(false);\r\n  const handleShow = () => setShow(true);\r\n  const [activeSpinner, setActiveSpinner] = useState(false);\r\n  // ----------- width size -----------------------\r\n  const [deviceSize, changeDeviceSize] = useState(window.innerWidth);\r\n  useEffect(() => {\r\n    const resizeW = () => changeDeviceSize(window.innerWidth);\r\n\r\n    window.addEventListener(\"resize\", resizeW); // Update the width on resize\r\n    return () => window.removeEventListener(\"resize\", resizeW);\r\n  });\r\n\r\n  const cookies = new Cookies();\r\n  var userEmail = cookies.get(\"email\");\r\n\r\n  let historyUrl = useHistory();\r\n\r\n  const getHistory = () => {\r\n    setActiveSpinner(true);\r\n    fetch(`https://winelovers.herokuapp.com/getHistory/${userEmail}`)\r\n      .then((res) => res.json())\r\n      .then((json) => {\r\n        setActiveSpinner(false);\r\n        setHistory(json);\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    getHistory();\r\n  }, []);\r\n\r\n  return (\r\n    <div>\r\n      <Modal show={show} onHide={handleClose} size=\"lg\">\r\n        <Modal.Body>\r\n          {orderSelected.length !== 0 && (\r\n            <>\r\n              <Row>\r\n                <Col>\r\n                  <h4>Order Details</h4>\r\n                  <br />\r\n                </Col>\r\n              </Row>\r\n              <Row>\r\n                <Col xs={deviceSize < 600 ? 12 : 6}>\r\n                  <BsInfoCircle className=\"history-icon\" /> Order Id :{\" \"}\r\n                  <strong>{orderSelected[0]._id}</strong>\r\n                </Col>\r\n                <Col xs={deviceSize < 600 ? 12 : 6}>\r\n                  <AiOutlineCalendar className=\"history-icon\" /> Order Date :{\" \"}\r\n                  <strong>{orderSelected[0].OrderDate.slice(0, 10)}</strong>\r\n                </Col>\r\n              </Row>\r\n              <Row>